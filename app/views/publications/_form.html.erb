<%= form_for @publication do |f| %>
  <%= f.text_field :title %><br />
  <%= f.text_field :onmain %><br />
  <%= f.select(:category_id, @categories.collect {|p| [ p.title, p.id ] }) %><br />
  <%= f.text_field :slug %><br />
  <%= f.text_area  :intro %><br />
  <%= f.text_area  :body %><br />
  <%= f.file_field :newspic %>
  <%= f.submit  %>
<% end %>


